<h1>profile PAGE :)</h1>

<% if(currentUser.local.username !== undefined) { %>
    <div>
        <%= currentUser.local.username %>
    </div>
<% } %>

<% if(currentUser.facebook.username !== undefined) { %>
    <div>
        <div>
            <img src="<%= currentUser.facebook.url %>" >
        </div>
        <div>
            <%= currentUser.facebook.username %>
        </div>
    </div>
<% } %>

<% if(currentUser.google.username !== undefined) { %>
    <div>
        <div>
            <img src="<%= currentUser.google.url %>" >
        </div>
        <div>
            <%= currentUser.google.username %>
        </div>
    </div>
<% } %>

<a href="/logout"><button>logout</button></a>
